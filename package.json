{
  "name": "family-tunes",
  "version": "1.0.0",
  "private": true,
  "description": "An interactive Spotify web app.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
